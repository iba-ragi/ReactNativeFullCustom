{
  "name": "subscriptionManager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "fmt": "prettier --write src/**/*.{js,tsx} ",
    "tsc": "tsc --watch",
    "dev:ios": "ENVFILE=.env.dev react-native run-ios",
    "dev:android": "ENVFILE=.env.dev react-native run-android",
    "clean": "watchman watch-del-all && lsof -n -P -i :8081 -t | xargs kill && rm -rf ~/Library/Developer/Xcode/DerivedData && sudo rm -rf '~/Library/Developer/Xcode/iOS\\ DeviceSupport/*' && rm -rf ios/build android/app/build node_modules ios/Pods && yarn install && cd ios && pod install && cd .."
  },
  "dependencies": {
    "@types/styled-components": "^4.1.18",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-config": "^0.11.7",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-reanimated": "^1.1.0",
    "react-navigation": "^3.11.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.16",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.60.2",
    "@types/react-test-renderer": "^16.8.3",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "^24.8.0",
